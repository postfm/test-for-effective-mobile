{
  "name": "test-for-effective-mobile",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "lint": "eslint src/ --ext .ts",
    "db:migrate": "npx prisma migrate dev --name --schema ./src/prisma/schema.prisma --skip-generate",
    "db:validate": "npx prisma validate --schema ./src/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "db:seed": "npx ts-node-dev ./src/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1"
  }
}
